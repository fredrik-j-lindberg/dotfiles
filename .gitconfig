[core]
	editor = code --wait
	excludesfile = ~/git/dotfiles/.gitignore_global
[alias]
	autoFixup = !git stash push --keep-index && git commit --fixup=$1 && git -c sequence.editor=: rebase -i --autosquash $1~1  && git stash pop && :
	interactiveFixup = !git stash push --keep-index && git commit --fixup=$1 && git rebase -i --autosquash $1~1 && git stash pop && :
